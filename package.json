{
  "name": "solana-exchange",
  "version": "0.0.1",
  "description": "Solana exchange wrapper for AMM",
  "private": true,
  "scripts": {
    "build": "nest build",
    "docs": "typedoc src --out docs",
    "generate": "ts-node src/utils/keypair.ts",
    "repl": "RUN_ENV=local pnpm run start:debug -- --entryFile repl",
    "migration:create": "prisma migrate dev --name",
    "migrate": "prisma generate && prisma migrate deploy",
    "start:debug": "RUN_ENV=local nest start --type-check --debug --watch",
    "start:local": "RUN_ENV=local nest start --watch",
    "start:dev": "RUN_ENV=dev nest start -b swc --type-check",
    "start": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:cov": "jest --coverage"
  },
  "engines": {
    "node": ">=23.1.0",
    "pnpm": ">=8.0.0"
  },
  "keywords": [],
  "author": "Benny",
  "license": "Apache-2.0",
  "dependencies": {
    "@coral-xyz/anchor": "0.29",
    "@lightprotocol/compressed-token": "^0.17.1",
    "@lightprotocol/stateless.js": "^0.17.1",
    "@metaplex-foundation/mpl-core": "^1.1.1",
    "@metaplex-foundation/mpl-token-metadata": "^3.3.0",
    "@metaplex-foundation/mpl-toolbox": "^0.9.4",
    "@metaplex-foundation/umi": "^0.9.2",
    "@metaplex-foundation/umi-bundle-defaults": "^0.9.2",
    "@metaplex-foundation/umi-web3js-adapters": "^0.9.2",
    "@nestjs/common": "^10.4.15",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.15",
    "@nestjs/devtools-integration": "^0.1.6",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.4.15",
    "@orca-so/common-sdk": "0.6.4",
    "@orca-so/whirlpools-sdk": "^0.13.12",
    "@prisma/client": "^6.1.0",
    "@raydium-io/raydium-sdk-v2": "0.1.95-alpha",
    "@solana/spl-token": "^0.4.9",
    "@solana/web3.js": "^1.98.0",
    "bcrypt": "^5.1.1",
    "bn.js": "^5.2.1",
    "bs58": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "decimal.js": "^10.4.3",
    "express": "^4.21.2",
    "ioredis": "^5.4.2",
    "kafkajs": "^2.2.4",
    "nest-winston": "^1.9.7",
    "passport": "^0.7.0",
    "passport-httpapikey": "^1.0.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "prisma": "^6.1.0",
    "prisma-class-generator": "^0.2.11",
    "rxjs": "^7.8.1",
    "typedoc": "^0.26.11",
    "uuid": "^11.0.3",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.26",
    "@nestjs/cli": "^10.4.9",
    "@nestjs/swagger": "^8.1.0",
    "@nestjs/testing": "^10.4.15",
    "@types/bcrypt": "^5.0.2",
    "@types/bn.js": "^5.1.5",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.9.0",
    "@types/passport-jwt": "^4.0.1",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  },
  "jest": {
    "preset": "ts-jest/presets/default-esm",
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {},
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
